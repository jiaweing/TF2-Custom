"UnlitGeneric"
{
	"$translucent" 1
	"$baseTexture" "console\background_gravelpit_widescreen"
	"$vertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1
	"%keywords" "tf"
	
	"Proxies"
		{
			"AnimatedTexture"
			{
				"animatedtexturevar" "$basetexture"
				"animatedtextureframenumvar" "$frame"
				"animatedtextureframerate" 15
			}
		}
}